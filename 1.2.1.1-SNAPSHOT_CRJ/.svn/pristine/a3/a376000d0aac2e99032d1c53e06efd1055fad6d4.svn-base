*{
	margin: 0px;
	padding: 0px;
	font-family: "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", "arial", "Tahoma", "SimSun", "sans-serif";
}
p{
 	display: block;
    -webkit-margin-before: 0px;
    -webkit-margin-after: 0px;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    word-break: break-all;
 }
	select{
		padding: 5px 0px;
   		border: none;
   		width: 100%;
   		-webkit-appearance: none;
   		background-color: white;
	}	
.header{
	.navdiv{
		background-color: #f9bf91;		
		.nav-pills{
			background-color: #f9bf91;
			display: -webkit-box;
	    	display: -webkit-flex;
	    	display: flex;
	    	text-align: center;
	    	.con{
	    		-webkit-box-flex: 1;
				-webkit-flex: 1;
				flex: 1;
				padding: 15px 0px;
				a {
					color:#f3eadc;
					font-size:16px;
					text-decoration: initial;
					padding: 8px 0px;
				}				
	    	}
	    	.right{
				border-right: 1px solid white;
			}
			li{					
			    list-style-type: none;				  				
			}
			.active{
				background-color:#f7ac6f;
				a{
					background-color: #f7ac6f !important;
		  			border-bottom: 3px solid #c17d46;
		  			border-radius:0px !important;
		  			color: #fff;
		  			text-decoration: initial;	  			
				}
			}
			a:hover{
				background-color: #f7ac6f !important;
	  			border-bottom: 3px solid #c17d46; 
	  			border-radius:0px !important;
	  			color: #fff;
	  			text-decoration: initial;	
			}
		}
	}
}	
.body{
	margin: 10px 0px;
	.active{
		display: block !important;	
	}
	.com{
		display: none;
	}
	.weui_cells {
	    overflow: hidden;
	    position: relative;
        border-bottom: 1px solid #e4e3e3;
	    width: 100%;
	    margin: 0 auto;
	    p {
		    width:100%;
		    padding:8px 0px;
		    display: inline-block;
		}
		.weui_cell {
		    padding: 0px 15px 0px;
		    position: relative;
		    display: -webkit-box;
		    display: -webkit-flex;
		    display: flex;
		    -webkit-box-align: center;
		    -webkit-align-items: center;
		    align-items: center;
		    .weui_cell_primary {
			    -webkit-box-flex: 1;
			    -webkit-flex: 1;
			    flex: 1;
			}
			
			.weui_cell_primary2 {
			    -webkit-box-flex: 2;
			    -webkit-flex: 2;
			    flex: 2;			   			    			    
			}
			.weui_cell_ft {
			    -webkit-box-flex: 2;
			    -webkit-flex: 2;
			    flex: 2;
			    color: #aba9a9;
		        text-align: right;
			}
			.weui_cell_ft2 {
			    -webkit-box-flex: 3;
			    -webkit-flex: 3;
			    flex:3;
		        text-align: left;
		         input{
		          text-align: left;
		         	width:100%;
			    	border:0px;
			    	-webkit-appearance: none;			    				    
			    };
			}
			.weui_cell_ft1 {
			    -webkit-box-flex: 1;
			    -webkit-flex: 1;
			    flex: 1;
			    color: #aba9a9;
			}
	    }
	}
	.division{
		display: -webkit-box;
	    display: -webkit-flex;
	    display: flex;
	    width: 100%;
	    padding: 35px 0px 10px;
		.left{
			-webkit-box-flex: 1;
		    -webkit-flex: 1;
		    flex: 1;		
			hr{
				border-bottom:1px dashed black;
				margin-top: 5px;
				width:60%;
				float:right;
				border-top: initial;
			}
		}
		.center{
			-webkit-box-flex: 1;
		    -webkit-flex: 1;
		    flex: 1;
		    text-align: center;
		    color:#ff6d01;
		    font-weight: bold;
		    .plus{
		    	font-size: 15px;
	    	    font-weight: 600;
		    }
		    .minus{
		    	font-size: 15px;
	    	    font-weight: 600;
		    }	    
		}
		.right{
			-webkit-box-flex: 1;
		    -webkit-flex: 1;
		    flex: 1;
		    hr{
				border-bottom:1px dashed black;
				margin-top: 5px;
				width:60%;
				float:left;
				border-top: initial;
			}
		}	
	}
	.nummber{
		padding: 0px 15px 0px;
		.row{
			display: -webkit-box;
		    display: -webkit-flex;
		    display: flex;
	        width: 95%;
    		margin: 0 auto;
			.leftdiv{
				-webkit-box-flex: 2;
		    	-webkit-flex: 2;
		    	flex: 2;
		    	.color1{
		    		color:#aeaeaf;
		    		font-size: 14px;
		    	}
			}
			.rightdiv{
				-webkit-box-flex: 1; 
			    -webkit-flex: 1;
			    flex: 1;
			    p{
			    	text-align:right;
			    	color:red;
			    }
			}
			.rightdiv1{
				-webkit-box-flex: 2; 
			    -webkit-flex: 2;
			    flex: 2;
			    p{			    	
			    	color:red;
			    	text-align:center;
			    }
			}
		}
	}
	.info{
		border-bottom:1px solid #eae9e9;
	    padding-bottom: 4px;
    	width: 95%;
    	margin: 0 auto;
    	padding-top: 10px;
		.headdiv{
			.list{		    
	   		    margin-left: 15px;
		        border: 1px solid #d6d2cf;
			    width: 28%;
			    border-radius: 10px;
				p{
			        text-align: center;
				    color:#ee7836;
			        padding: 1px;
				}
			}		
		}
		.content{
			margin-top: 3px;
			.detail{
				display: -webkit-box;
			    display: -webkit-flex;
			    display: flex;
				.left{
					-webkit-box-flex: 2; 
			    	-webkit-flex: 2;
			    	flex: 2;
			    	margin-left: 15px;
			    	img{
			    		padding: 2px 0px 0px;
			    	}  
				}
				.right{
					-webkit-box-flex: 3; 
					text-align: right;
			    	-webkit-flex: 3;
			    	margin-right: 15px;
			    	flex: 3;
			    	color: #aba9a9;
				}
			}
			.hide{
				display:none;
				margin: 0 15px;
				color: #f7ac6f;
			}
		}
	}
}