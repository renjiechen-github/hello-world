package pccom.web.flow.util;

/**
 * 步骤中对应的状态信息
 * @author 雷杨
 *
 */
public class FlowStepState {
	/**
	 * 刚创建步骤
	 */
	public static final String CREATE_STEP = "0";
	
	/**
	 * 已阅读步骤
	 */
	public static final String READ_STEP = "1";
	
	/**
	 * 完成当前步骤
	 */
	public static final String END_STEP = "2";
	
	/**
	 * 回退当前步骤
	 */
	public static final String ROLLBACK_STEP = "3";
	
	/**
	 * 挂起当前步骤
	 */
	public static final String HANG_STEP = "4";
	
	/**
	 * 当前步骤等待子任务结束完成中
	 */
	public static final String WAIT_STEP = "5";
	
	/**
	 * 修改当前步骤，不提交订单
	 */
	public static final String MODIFY_STEP = "6";
	
	/**
	 * 关闭订单
	 */
	public static final String CLOSE_STEP = "8";
	
	/**
	 * 订单跟进
	 */
	public static final String FOLLOW_UP = "9";
}
