package com.ycdc.dingding.model.entity;

/**
 * @name: HomeDevice.java
 * @Description: 
 * @author duanyongrui
 * @since: 2017年5月22日
 */
public class HomeDevice {
	/**
	 * 设备类型，取值：
	 * gateway:网关
	 * home_lock:外门锁 
	 * room_lock:内门锁
	 * elemeter: 电表 
	 * elecollector: 采集器
	 */
	private String type;
	/**
	 * 设备序列号
	 */
	private String sn;
	/**
	 * 设备uuid
	 */
	private String uuid;
	/**
	 * 房间id（外门锁的房间id等于home_id）
	 */
	private String room_id;
	/**
	 * 设备描述（可选）
	 */
	private String description;
	/**
	 * 网管中心uuid(可选)
	 */
	private String center_uuid;
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getSn() {
		return sn;
	}
	public void setSn(String sn) {
		this.sn = sn;
	}
	public String getUuid() {
		return uuid;
	}
	public void setUuid(String uuid) {
		this.uuid = uuid;
	}
	public String getRoom_id() {
		return room_id;
	}
	public void setRoom_id(String room_id) {
		this.room_id = room_id;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getCenter_uuid() {
		return center_uuid;
	}
	public void setCenter_uuid(String center_uuid) {
		this.center_uuid = center_uuid;
	}
	
}
