	            		<div class="weui_cell_ft"><input type="number" name="eMeterH"  dataType="Integer2" msg="请输入电表峰值！" placeholder="请输入电表峰值"  id="eMeterH" value="" class="form-control inputChange2"/></div>
	        		</div>
    			</div>
    			<div class="weui_cells">
	        		<div class="weui_cell">
			            <div class="weui_cell_bd weui_cell_primary">
			                <p>电表谷值</p>
			            </div>
	            		<div class="weui_cell_ft"><input type="number" name="eMeterL"  dataType="Integer2" msg="请输入电表谷值！" placeholder="请输入电表谷值"  id="eMeterL" value="" class="form-control inputChange2"/></div>
	        		</div>
    			</div>
    			<div class="weui_cells">
	        		<div class="weui_cell">
			            <div class="weui_cell_bd weui_cell_primary">
			                <p>水卡账号</p>
			            </div>
	            		<div class="weui_cell_ft"><input type="text" name="cardWarter"  dataType="Require" msg="请输入水卡账号！" placeholder="请输入水卡账号"  id="cardWarter" value="" class="form-control inputChange2"/></div>
	        		</div>
    			</div>
    			<div class="weui_cells">
	        		<div class="weui_cell">
			            <div class="weui_cell_bd weui_cell_primary">
			                <p>水表读数</p>
			            </div>
	            		<div class="weui_cell_ft"><input type="number" name="warterDegrees"  dataType="Integer2" msg="请输入水表读数！" placeholder="请输入水表读数"  id="warterDegrees" value="" class="form-control inputChange2"/></div>
	        		</div>
    			</div>
    			<div class="weui_cells">
	        		<div class="weui_cell">
			            <div class="weui_cell_bd weui_cell_primary">
			                <p>燃气账号</p>
			            </div>
	            		<div class="weui_cell_ft"><input type="text" name="cardgas"  placeholder="请输入燃气帐号"  id="cardgas" value="" class="inputChange2"/></div>
	        		</div>
    			</div>
    			<div class="weui_cells">
	        		<div class="weui_cell">
			            <div class="weui_cell_bd weui_cell_primary">
			                <p>燃气读数</p>
			            </div>
	            		<div class="weui_cell_ft"><input type="number" name="gasDegrees" placeholder="请输入燃气读数"  id="gasDegrees" value="" class="inputChange2"/></div>
	        		</div>
    			</div>
				<div class="textarea">
					<textarea rows="5" class="inputChange2" id="descInfo" placeholder="请输入合约描述" name="descInfo"></textarea>
				</div>
				<div class="upload">
	        		<div class="weui_cell file" onclick="house_agreement_info.selectImages('selectImage1');return false;">
	        			<a href="" style="text-decoration: none;color: black;">选择房产证附件上传</a> 
	        		</div>
	        		<ul class="ipost-list ui-sortable js_fileList" id="selectImage1"></ul> 
	        		<input type="hidden" value="4" name="picSize" id="picSize" /> 
	        		<input type="hidden" value="" name="picPath" id="picPath" /> 	        		  
	    		</div> 		
				<div class="upload">
	        		<div class="weui_cell file"  onclick="house_agreement_info.selectImages('selectPropertyPath');return false;">
	        			<a href="" style="text-decoration: none;color: black;">选择产权人附件上传</a> 
	        		</div>
	        		<ul class="ipost-list ui-sortable js_fileList" id="selectPropertyPath"></ul> 
	        		<input type="hidden" value="" name="propertyPath" id="propertyPath" /> 	        		  
	    		</div> 		
				<div class="upload">
	        		<div class="weui_cell file"  onclick="house_agreement_info.selectImages('selectAgentPath');return false;">
	        			<a href="" style="text-decoration: none;color: black;">选择代理人附件上传</a> 
	        		</div>
	        		<ul class="ipost-list ui-sortable js_fileList" id="selectAgentPath"></ul> 
	        		<input type="hidden" value="" name="agentPath" id="agentPath" /> 	        		  
	    		</div> 		
	    	</div>
		</div>
	</div>
