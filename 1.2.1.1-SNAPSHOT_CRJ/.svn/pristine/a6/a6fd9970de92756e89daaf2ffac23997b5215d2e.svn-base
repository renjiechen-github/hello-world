<form action="" id="form1">	
<div class="body">
	<div class="cons1">
		<div class="weui_cells">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>房源名称</p>
	            </div>
	            <div class="weui_cell_ft houseName"></div>
          		<input type="hidden" value="" name="houseId" id="houseId" />
				<input type="hidden" value="" name="user_id" id="user_id" />
				<input type="hidden" value="" name="user_mobile" id="user_mobile" />
				<input type="hidden" value="" name="areaid" id="areaid" />
				<input type="hidden" value="" name="houseCode" id="houseCode" />
				<input type="hidden" value="" name="agreementId" id="agreementId" />
	        </div>
    	</div>
    	<div class="weui_cells">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>业主信息</p>
	            </div>
	            <div class="weui_cell_ft yzInfo"></div>
	        </div>
    	</div>	
    	<div class="weui_cells">
				<div class="weui_cell">
					<div class="weui_cell_bd weui_cell_primary">
						<p>身份证号</p>
					</div>
					<div class="weui_cell_ft">
						<input id="certificateno" name="certificateno" dataType="IdCard" msg="请输入有效身份证号！"
							placeholder="请输入有效身份证号" class="inputChange2"
							type="text" />
					</div>
				</div>
			</div>	
    	<div class="weui_cells" id="aggreenDiv" style="display: none;">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>合约编码</p>
	            </div>
	            <div class="weui_cell_ft"><input type="text"  name="agreementInput" id="agreementInput" readonly="readonly"/></div>
	        </div>
    	</div>    	
    	<div class="weui_cells">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>合约名称</p>
	            </div>
	            <div class="weui_cell_ft"><input class="inputChange2" type="text" name="contractName" dataType="Custom" regexp="^([\u4e00-\u9fa5A-Za-z0-9-_]{1,})$" msg="请输入合法的合约名称！" id="contractName" placeholder="请输入合约名称" /></div>
	        </div>
    	</div>    	
    	<div class="weui_cells" style="display: none;">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>合约编号</p>
	            </div>
	            <div class="weui_cell_ft"><input type="number" class="inputChange2" name="contractId" id="contractId"  placeholder="请输入合约编号"/></div>
	        </div>
    	</div>
    	<div class="weui_cells">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>签约管家</p>
	            </div>
	            <div class="weui_cell_ft">
	            	<select class="color" id="contractManage" dataType="Require" msg="请选择签约管家！" name="contractManage">
					</select>
				</div>
	        </div>
    	</div>	
    	<div class="weui_cells">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>是否可隔</p>
	            </div>
	            <div class="weui_cell_ft">
	            	<select class="color" name="is_kg" id="is_kg" dataType="Require" msg="请选择是否可以隔离！">
						<option value="0">否</option>
						<option value="1">是</option>
					</select>
				</div>
	        </div>
    	</div>
	 	<div class="weui">
	       	<div class="weui_cell">
		       	<div class="row">
				   	<div class="common"><input type="number" name="doorkey"  dataType="Integer2" msg="请输入门禁钥匙！" placeholder="请输入门禁钥匙"  id="doorkey" value="" class="inputChange2"/></div>
				   	<div class="common"><input type="number" name="fooler"  dataType="Integer2" msg="请输入数字格式的楼层！" placeholder="请输入楼层"  id="fooler" value="" class="inputChange2"/></div>
				   	<div class="common"><input type="number" name="personCount"  dataType="Integer2" msg="请输入入户！" placeholder="请输入入户"  id="personCount" value="" class="form-control inputChange2"/></div>	 
		   		</div>
	   		</div>
	   	</div>
    	<div class="weui_cells">
	        <div class="weui_cell2">
	            <div class="weui_cell_bd weui_cell_primary2">
	                <p>房间配置</p>
	            </div>
	            <div class="weui_cell_ft2" id="div_room_pz"></div>
	        </div>
	     </div>
	     <div class="weui_cells">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>付款方式</p>
	            </div>
	            <div class="weui_cell_ft"><select class="color" name="payType" class="changeInput" id="payType"></select></div>
	        </div>
	     </div>
	     <div class="weui_cells">	     	
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>免租期(天)</p>
	            </div>
	            <div class="weui_cell_ft"><input type="number" name="fee_date"  dataType="Integer2" msg="请输入免租期！" placeholder="请输入免租期"  id="fee_date" value="0" class="inputChange2"/></div>
	        </div>
	     </div>
	     <div class="weui_cells">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>生效时间</p>
	            </div>
	            <div class="weui_cell_ft"><input type="text" dataType="Require" msg="请选择生效时间！"  placeholder="请选择生效时间"  class="inputChange2" readonly="readonly" name="effectDate" id="effectDate" /></div>
	        </div>
	     </div>
	     <div class="weui_cells">
	        <div class="weui_cell">
	            <div class="weui_cell_bd weui_cell_primary">
	                <p>租约时长</p>
	            </div>
	            <div class="weui_cell_ft"><select class="color" name="leaseTime"  dataType="Require" msg="请选择租约时长！"  id="leaseTime"></select></div>
	        </div>
    	</div>	
    	<div class="weui" id="div_rent">
	       	<div class="weui_cell">
		       	<div class="row">
				   	<div class="common"><input name="rentMonth" class="right" type="number" placeholder="1"></div>
				   	<div class="common"><input name="rentMonth" class="right" type="number" placeholder="2"></div>
				   	<div class="common"><input name="rentMonth" class="right" type="number" placeholder="3"></div>
				   	<div class="common"><input name="rentMonth"  type="number" placeholder="4"></div>	 
		   		</div>
		   	</div>
	   	</div> 	    
	</div>				
	<div class="cons1">
		<div class="detail2">
			<div class="">
