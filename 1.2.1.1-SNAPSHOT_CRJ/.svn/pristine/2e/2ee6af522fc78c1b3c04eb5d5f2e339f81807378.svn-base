package pccom.web.flow.util;

/**
 * 流程主线任务状态定义
 * @author 雷杨
 *
 */
public class FlowTaskState {
	/**
	 * 任务创建成功
	 */
	public static final String CREATE_TASK = "0";
	
	/**
	 * 开始任务
	 */
	public static final String START_TASK = "1";
	
	/**
	 * 中断任务
	 */
	public static final String BREAK_TASK = "2";
	
	/**
	 * 恢复任务
	 */
	public static final String RECOVER_TASK = "3";
	
	/**
	 * 任务完成
	 */
	public static final String OVER_TASK = "4";
}
