<body>
	<section class="panel">
		<form id="form2" class="form-horizontal adminex-form " role="form">
			<!-- 基本信息-->
			<div class="form-group">
				<label class="col-sm-2  control-label"></label>
				<div class="col-sm-6"></div>
				<div class="col-sm-4"></div>
			</div>
			<div class="form-group">
				<label class="col-sm-2  control-label">订单编号：</label>
				<div class="col-sm-3">
					<span id="order_code" class="form-control" style="border: 0px"></span>
				</div>
        <label class="col-sm-2  control-label"><b style="color: red">*</b>订单名称：</label>
        <div class="col-sm-3">
          <input maxlength="50" id="orderName" msg="请填写订单名称" type="text" placeholder="建议保留系统生成名称" class="form-control" dataType="Require"></input>
        </div>  				
				<div class="col-sm-2"></div>
			</div>
			<div class="form-group">
				<label class="col-sm-2  control-label">联系方式：</label>
				<div class="col-sm-3">
					<span id="ouserMobile" msg="请输入正确的手机号码" class="form-control" style="border: 0px" dataType="Number"></span>
				</div>
				<label class="col-sm-2  control-label">用户名称：</label>
				<div class="col-sm-3">
					<span id="ouserName" class="form-control" style="border: 0px" dataType="Require"></span>
				</div>
				<div class="col-sm-2"></div>
			</div>
			<div class="form-group">
				<label class="col-sm-2  control-label"><b style="color: red">*</b>预约时间：</label>
				<div class="col-sm-3">
					<input msg="请选择预约时间" readonly="readonly" dataType="Require" type="text" id="oserviceTime" class="form-control" onFocus="WdatePicker({startDate:'%y-%M-%d %H:%m:%s',dateFmt:'yyyy-MM-dd HH:mm',alwaysUseStartDate:true,readOnly:true})" />
				</div>
        <label class="col-sm-2  control-label">订单状态：</label>
        <div class="col-sm-3">
          <span id="order_status" class="form-control" style="border: 0px"></span>
        </div>				
				<div class="col-sm-2"></div>
			</div>
			<div class="hidden form-group js-div-upload">
				<label class="col-sm-2  control-label">图片上传：</label>
				<div class="col-sm-8">
					<div class="dropzone" id="upurl"></div>
					<div class="row"></div>
				</div>
				<div class="col-sm-2"></div>
			</div>
			<div class="form-group">
				<label class="col-sm-2  control-label">备注：</label>
				<div class="col-sm-8">
					<textarea id="odesc" rows="6" maxlength="255" class="form-control" placeholder="请填写订单相关信息"></textarea>
				</div>
				<div class="col-sm-2"></div>
			</div>
			<div class="form-group">
				<label class="col-sm-2  control-label"></label>
				<div class="col-sm-6"></div>
				<div class="col-sm-4"></div>
			</div>
			<div class="text-center">
				<button class="btn btn-info" id="orderadd_bnt" onclick="jsOrderUpdateDetail.saveData()" type="button">保存</button>
			</div>
		</form>
	</section>
</body>
