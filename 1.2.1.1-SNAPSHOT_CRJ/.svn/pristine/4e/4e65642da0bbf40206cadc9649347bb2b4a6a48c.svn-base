package com.ycdc.dingding.model.entity;

/**
 * @name: SearchHomeCondition.java
 * @Description: 
 * @author duanyongrui
 * @since: 2017年5月22日
 */
public class SearchHomeCondition {
	/**
	 * 房源编号(支持模糊匹配)
	 */
	private String home_name;
	/**
	 * 小区(支持模糊匹配)
	 */
	private String block;
	/**
	 * 位置(支持模糊匹配)
	 */
	private String location;
	/**
	 * 拉取的偏移量，默认为 0
	 */
	private int offset;
	/**
	 * 本次拉取的个数，默认是 20
	 */
	private int count = 20;
	public String getHome_name() {
		return home_name;
	}
	public void setHome_name(String home_name) {
		this.home_name = home_name;
	}
	public String getBlock() {
		return block;
	}
	public void setBlock(String block) {
		this.block = block;
	}
	public String getLocation() {
		return location;
	}
	public void setLocation(String location) {
		this.location = location;
	}
	public int getOffset() {
		return offset;
	}
	public void setOffset(int offset) {
		this.offset = offset;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	/**
	 * 
	 */
	public SearchHomeCondition() {
		super();
	}
	/**
	 * @param home_name
	 * @param block
	 * @param location
	 * @param offset
	 * @param count
	 */
	public SearchHomeCondition(String home_name, String block, String location, int offset, int count) {
		super();
		this.home_name = home_name;
		this.block = block;
		this.location = location;
		this.offset = offset;
		this.count = count;
	}
	
	
}
