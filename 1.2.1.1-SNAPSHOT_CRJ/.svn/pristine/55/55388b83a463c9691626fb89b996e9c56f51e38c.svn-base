package com.ycdc.dingding.model.entity;

/**
 * @name: LockOperation.java
 * @Description: 
 * @author duanyongrui
 * @since: 2017年5月23日
 */
public class LockOperation {
	/**
	 * 操作类型 id
	 */
	private String op_id;
	/**
	 * 操作人
	 */
	private LockOperator operator;
	/**
	 * 操作对象，如房间
	 */
	private LockTarget target;
	/**
	 * 操作对象的容器，如，对于房间 holder 是公寓
	 */
	private LockHolder holder;
	/**
	 * 操作的‘受益人’，如发送房间 1 的密码给 小 K，小 K 就是这里的 recipient
	 */
	private LockRecipient recipient;
	/**
	 * 操作时间时间戳，单位 ms
	 */
	private long timestamp;
	public String getOp_id() {
		return op_id;
	}
	public void setOp_id(String op_id) {
		this.op_id = op_id;
	}
	public LockOperator getOperator() {
		return operator;
	}
	public void setOperator(LockOperator operator) {
		this.operator = operator;
	}
	public LockTarget getTarget() {
		return target;
	}
	public void setTarget(LockTarget target) {
		this.target = target;
	}
	public LockHolder getHolder() {
		return holder;
	}
	public void setHolder(LockHolder holder) {
		this.holder = holder;
	}
	public LockRecipient getRecipient() {
		return recipient;
	}
	public void setRecipient(LockRecipient recipient) {
		this.recipient = recipient;
	}
	public long getTimestamp() {
		return timestamp;
	}
	public void setTimestamp(long timestamp) {
		this.timestamp = timestamp;
	}
	
}
