*{
	margin: 0px;
	padding: 0px;
	font-family: 微软雅黑;
}
a {
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
}
#banner{
    width: 100%;
    text-align: center;
    position: fixed;
    box-sizing: border-box;
    padding: 15px 10px;
    background-color: #bbbbbb;   
    top:0px;
    left:0px;
    padding-top:30px;
    .inpy{ 
	    margin-left: auto;
	    margin-right: auto;
	    display: -webkit-box;
	    display: -webkit-flex;
	    display: flex;
	    .button-group{
	    	-webkit-box-flex: 3;
			-webkit-flex: 3;
			flex: 3;
			border: 1px solid black;
		    select{
		    	border-radius: initial;
		    	width:100%;
			    padding: 5px;
			    border: 0px;
			    color: black;
			    font-family: 微软雅黑;
			    height: 31px;
			    font-size: 15px;
			    -webkit-appearance: listbox;
		     }
	    }   
		.inputdiv{
			-webkit-box-flex: 6;
			-webkit-flex: 6;
			flex: 6;
			border-top: 1px solid black;
			border-bottom: 1px solid black;
			input{
				border-radius: initial;
				width:100%;
				padding-left: 5px;
			    border-right: 0px;
			    border-left: 0px;
			    border: 0px;
			    cursor: pointer;
			    color: black;
			    height: 31px;
			    box-shadow: none;
			}
		}
		.buttondiv{
			-webkit-box-flex: 3;
			-webkit-flex: 3;
			flex: 3;
			.button{
				cursor: pointer;
				border-color: black;
		    	background-color: black;  
		    	border: none;
		    	font-family: 微软雅黑;
		    	font-size: 15px;
		    	color: white;
		    	height: 33px;
		    	width:100%;
		    	padding: 3px;
			}			
		}
		.rightdiv{
			-webkit-box-flex: 1;
		    -webkit-flex: 1;
		    flex: 1;
		    height: 33px;
		    a{
		    	text-decoration: initial;
		    }		    
		}
	}
	.hide{		
		display: none;
	    width: 80%;
    	margin-top: 15px;
    	margin-bottom: 5px;
   	    margin-left: auto;
    	margin-right: auto;
	    .hidediv{
   			display: -webkit-box;
	    	display: -webkit-flex;
	    	display: flex;
	    	margin-top: 10px;
	    }
		select{
			border-radius: initial;
		    border: 0px;
		    color: white;
		    background-color: #bbbbbb;
		    font-size: 15px;
		    -webkit-appearance: initial;
		    text-align: center;
		}
		em{
		    font-style: normal;
    		color: white;
    		margin: 0 10px;
		}
		.divflex{
			-webkit-box-flex: 3;
			-webkit-flex: 3;
			flex: 3;
		}
		.divflex1{
			-webkit-box-flex: 1;
			-webkit-flex: 1;
			flex: 1;
		}
		.divflex2{
			-webkit-box-flex: 3;
			-webkit-flex: 3;
			flex: 3;
		}
		.divflex3{
			-webkit-box-flex: 3;
			-webkit-flex: 3;
			flex: 3;
			label{
				color: white;
			}
		}
		.divflex4{
			-webkit-box-flex: 3;
			-webkit-flex: 3;
			flex: 3;
		}
		.empty{
			-webkit-box-flex: 3;
			-webkit-flex: 3;
			flex: 3;
		}		
	}
}
.body{	
	font-size: 14px;
	padding: 0px 2%;
	box-sizing: border-box;
    padding-top: 85px;
    a{    	
		text-decoration:none;
		color:#717173;
		-webkit-tap-highlight-color:rgba(0,0,0,0);
		.div{
			margin-bottom:5px;
			.space{
			padding-bottom:5px !important;
			}
			.con{
				padding: 0px 15px 10px;
				display: -webkit-box;
				display: -webkit-flex;
				display: flex;
				p{
					overflow: hidden;
				    text-overflow: ellipsis;
				    display: -webkit-box;
				    -webkit-box-orient: vertical;
				    -webkit-line-clamp: 1;
				}
				.info{
					-webkit-box-flex: 7;
					-webkit-flex: 7;
					flex: 7;
					color:#717173;
					padding: 5px 0px 0px;
					padding-right: 10px;
					p{
						font-weight: bold;
					}
				}
				.state{
					-webkit-box-flex: 1;
					-webkit-flex: 1;
					flex: 1;
					color:#717173;
					padding: 5px 0px 0px;
					.mark{
						float: right;
						background-image: url(../../images/market/up.png);
					    width: 16px;
    					height: 16px;
					}
					p{
						float:left;
						color:#ff0000;
					}
					.mark1{
						float: right;
						background-image: url(../../images/market/down.png);
					    width: 16px;
    					height: 16px;
					}
				}
				.info1{
					-webkit-box-flex: 1;
					-webkit-flex: 1;
					flex: 1;
					color:#717173;
					padding: 1px 0px;
					padding-right: 10px;
				}
				.state1{
					-webkit-box-flex: 1;
					-webkit-flex: 1;
					flex: 1;	
					padding: 1px 0px;					
				}
			}
		}
		.housecolor0{
			color:#F8B62D;//待提交
		}
		.housecolor1{
			color:#ED7210;//待审批
		}
		.housecolor2{
			color:#EB624D;//待签约
		}
		.housecolor3{
			color:#C9478A;//签约中
		}
		.housecolor4{
			color:#3E82BC;//待开工
		}
		.housecolor5{
			color:#009FA8;//施工中
		}
		.housecolor6{
			color:#45B78D;//待交接
		}
		.housecolor9{
			color:#A94F9A;//已生效
		}
		.housecolor10{
			color:#f14040;//已失效
		}
		.publishcolor0{
			color:#009FAB;//未发布
		}
		.publishcolor1{
			color:#A94F9A;//发布待审批
		}
		.publishcolor2{
			color:#45B78D;//已发布
		}		
		hr{
			border-bottom: 1px solid #dcdbdb;
    		border-top: none;
    		width: 95%;
    		margin: 0 auto;
		}
		.state2{
			-webkit-box-flex: 9;
			-webkit-flex: 9;
			flex: 9;
			color:#717173;
			p{
				color:#717173;
			}
		}
		.state3{
			-webkit-box-flex: 2;
			-webkit-flex: 2;
			flex: 2;
			color:#a98bd9;
			padding: 1px 0px;
			p{
				float:right;
			}
		}	 
		.detail{
			color:#717173;
			padding: 0px 15px;
			em{
				font-style: normal;
				color:#adadaf; 
			} 
			.left{
				float:left;			
			}
			.color1{
				color:#aba9a9;
				span{
					color:#717173;
				}
			}			
		}
		.color2{
			color:#aba9a9;
		    padding: 1px 0px;
			span{
				color:#a22d10;
			}
		}	
	}	
}
.rot2{
	-moz-transform:rotate(180deg);
	 -webkit-transform:rotate(180deg); 
	 transform:rotate(180deg); 
	 filter:progid:DXImageTransform.Microsoft.BasicImage(rotation=2); 
	 -ms-filter:progid:DXImageTransform.Microsoft.BasicImage(rotation=2);
}
.divCenter{
	width: 100%;
	height: 100%;
	opacity: 0.9;
	background: #b3aeae;
	z-index: 1000;
	position: fixed;
	display: none;
}
.cirleDiv{
	border-radius: 10px;
	height: 50px;
	width: 65%;
	line-height:50px;
	background-color: #ff0000;
	text-align: center;
	margin: 0 auto;
	background-color: #2981e4;
}
.bgDiv{
	top: 30%;
	position: relative;
	width: 100%;
}
.fixdiv{
	background-color: #fd9054;
    position: fixed;
    bottom: 15px;
    right: 15px;
    border-radius:30px;
    box-shadow: 2px 3px 10px #929496;
	img{
	    width: 30px;
    	height: 30px;
    	padding: 8px 10px;
	}	
}
